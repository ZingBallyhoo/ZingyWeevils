{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Stl": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Proto": "Warning",
      "BinWeevils.Server.UsernameOnlyAuthenticationHandler": "Information"
    },
    "Console": {
      "IncludeScopes": false
    },
    "OpenTelemetry": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "AllowedHosts": "*"
}
